{
	"name": "eventum",
	"description": "Eventum Composer Repository",
	"homepage": "https://eventum.github.io/composer/",
	"include-filename": "all.json",
	"repositories": [
		{
			"type": "pear",
			"url": "https://pear.php.net",
			"vendor-alias": "pear"
		},
		{
			"type": "vcs",
			"url": "https://github.com/glensc/zend-mail"
