		},
		{
			"type": "vcs",
			"url": "https://github.com/glensc/zend-mail"
		},
		{
			"type": "package",
			"package": {
				"name": "sphinx/php-sphinxapi",
				"version": "2.0.1",
				"license": [
					"GPL-2.0"
				],
				"dist": {
					"url": "https://raw.githubusercontent.com/sphinxsearch/sphinx/3b4cbb2b20e03e4d04db0695947441c62034aeec/api/sphinxapi.php",
					"shasum": "2b1274cbbc4b2be2aba2cb90dc5daf2036df0f49",
					"type": "file"
				},
				"autoload": {
					"classmap": [ "sphinxapi.php" ]
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "php-gettext/php-gettext",
				"description": "Pure PHP Implementation of gettext",
				"version": "1.0.11",
				"license": [
					"GPL-2.0", "GPL-3.0"
				],
				"dist": {
					"url": "https://launchpad.net/php-gettext/trunk/1.0.11/+download/php-gettext-1.0.11.tar.gz",
					"shasum": "9e6f402b460ba5e691e6e21f95d23527ab965f71",
					"type": "tar"
				},
				"provide":  {
					"ext-gettext":"self.version"
				},
				"autoload": {
					"classmap": [ "gettext.php", "streams.php" ]
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "components/jquery-chosen",
				"type": "component",
				"version": "1.2.0",
				"license": [
					"MIT"
				],
				"dist": {
					"url": "https://github.com/harvesthq/chosen/releases/download/v1.2.0/chosen_v1.2.0.zip",
					"type": "zip"
				},
				"extra": {
					"component": {
						"scripts": [
							"chosen.jquery.js"
						],
						"style": [
							"chosen.css"
						],
						"files": [
							"*.png",
							"chosen.css"
						]
					}
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "enyo/dropzone",
				"type": "component",
				"version": "4.0.1",
				"license": [
					"MIT"
				],
				"dist": {
					"url": "https://github.com/enyo/dropzone/archive/v4.0.1.zip",
					"type": "zip"
				},
				"extra": {
					"component": {
						"scripts": [ "index.js", "dist/dropzone.js" ],
						"styles": [ "dist/basic.css" ]
					}
				}
			}
		},
		{
			"type": "vcs",
			"url": "https://github.com/malsup/blockui"
