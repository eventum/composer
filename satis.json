{
	"name": "eventum",
	"description": "Eventum Composer Repository",
	"repositories": [
		{
			"type": "pear",
			"url": "http://pear.php.net",
			"vendor-alias": "pear"
		},
		{
			"type": "vcs",
			"url": "https://github.com/eventum/rpc"
		},
		{
			"type": "package",
			"package": {
				"name": "smarty/smarty",
				"version": "v3.1.21",
				"dist": {
					"url": "http://www.smarty.net/files/Smarty-3.1.21.zip",
					"type": "zip"
				},
				"require": {
					"php": ">=5.2"
				},
				"autoload": {
					"classmap": [
						"libs/Smarty.class.php"
					]
				},
				"license": [
					"LGPL-3.0"
				],
				"authors": [
					{
						"name": "Monte Ohrt",
						"email": "monte@ohrt.com"
					},
					{
						"name": "Uwe Tews",
						"email": "uwe.tews@googlemail.com"
					},
					{
						"name": "Rodney Rehm",
						"email": "rodney.rehm@medialize.de"
					}
				],
				"description": "Smarty - the compiling PHP template engine",
				"homepage": "http://www.smarty.net",
				"keywords": [
					"templating"
				]
			}
		},
		{
			"type": "package",
			"package": {
				"name": "sphinx/php-sphinxapi",
				"version": "2.0.1",
				"license": [
					"GPL-2.0"
				],
				"dist": {
					"url": "http://sphinxsearch.googlecode.com/svn/tags/REL_2_0_1/api/sphinxapi.php",
					"shasum": "2b1274cbbc4b2be2aba2cb90dc5daf2036df0f49",
					"type": "file"
				},
				"autoload": {
					"classmap": [ "sphinxapi.php" ]
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "php-gettext/php-gettext",
				"version": "1.0.11",
				"license": [
					"GPL-2.0", "GPL-3.0"
				],
				"dist": {
					"url": "https://launchpad.net/php-gettext/trunk/1.0.11/+download/php-gettext-1.0.11.tar.gz",
					"shasum": "9e6f402b460ba5e691e6e21f95d23527ab965f71",
					"type": "tar"
				},
				"provide":  {
					"ext-gettext":"self.version"
				},
				"autoload": {
					"classmap": [ "gettext.php", "streams.php" ]
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "components/jquery-chosen",
				"type": "component",
				"version": "1.2.0",
				"dist": {
					"url": "https://github.com/harvesthq/chosen/releases/download/v1.2.0/chosen_v1.2.0.zip",
					"type": "zip"
				},
				"extra": {
					"component": {
						"scripts": [
							"chosen.jquery.js"
						],
						"style": [
							"chosen.css"
						],
						"files": [
							"*.png",
							"chosen.css"
						]
					}
				},
				"require": {
					"robloach/component-installer": "*"
				}
			}
		},
		{
			"type": "vcs",
			"url": "https://github.com/malsup/blockui"
		},
		{
			"type": "package",
			"package": {
				"name": "components/jquery-datatables",
				"type": "component",
				"version": "1.10.4",
				"dist": {
					"url": "https://github.com/DataTables/DataTables/archive/1.10.4.zip",
					"type": "zip"
				},
				"extra": {
					"component": {
						"scripts": [
							"media/js/jquery.dataTables.js",
							"media/js/jquery.dataTables.min.js"
						],
						"style": [
							"media/css/jquery.dataTables.css",
							"media/css/jquery.dataTables.min.css"
						],
						"files": [
							"media/css/jquery.dataTables.css",
							"media/css/jquery.dataTables.min.css",
							"media/images/*.ico",
							"media/images/*.png"
						]
					}
				},
				"require": {
					"robloach/component-installer": "*"
				}
			}
		}
	],
	"require-dependencies": true,
	"require": {
		"components/jquery-blockui": "*@dev",
		"components/jquery-chosen": "*",
		"components/jquery-datatables": "*",
		"eventum/rpc": "*",
		"pear-pear.php.net/Auth_SASL": "*",
		"pear-pear.php.net/DB": "*",
		"pear-pear.php.net/File_Util": "*",
		"pear-pear.php.net/Mail": "*",
		"pear-pear.php.net/Mail_Mime": "*",
		"pear-pear.php.net/Mail_mimeDecode": "*",
		"pear-pear.php.net/Math_Stats": "*",
		"pear-pear.php.net/Net_LDAP2": "*",
		"pear-pear.php.net/Net_POP3": "*",
		"pear-pear.php.net/Net_SMTP": "*",
		"pear-pear.php.net/Net_SmartIRC": "*",
		"pear-pear.php.net/Net_Socket": "*",
		"pear-pear.php.net/Net_URL": "*",
		"pear-pear.php.net/PEAR": "*",
		"pear-pear.php.net/Text_Diff": "*",
		"pear-pear.php.net/XML_RPC": "*",
		"php-gettext/php-gettext": "*",
		"smarty/smarty": "*",
		"sphinx/php-sphinxapi": "*"
	}
}
