					"url": "https://downloads.sourceforge.net/phplot/phplot-6.1.0.tar.gz",
					"type": "tar"
				},
				"description": "PHPlot - Create charts in PHP",
				"keywords": [ "phplot", "chart" ],
				"homepage": "http://phplot.sourceforge.net",
				"license": "LGPL v2.1",
				"require": {
					"ext-date": "*",
					"ext-gd": "*",
					"ext-pcre": "*",
					"php": ">=5.3.0"
				},
				"autoload": {
					"classmap": ["phplot.php"]
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "jackmoore/autosize",
				"type": "component",
				"version": "3.0.13",
				"license": [
					"MIT"
				],
				"dist": {
					"url": "https://github.com/jackmoore/autosize/archive/3.0.13.zip",
					"type": "zip"
				},
				"extra": {
					"component": {
						"scripts": [ "dist/autosize.js" ]
					}
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "fonts/liberation",
				"version": "2.00.1",
				"license": "OFL-1.1",
